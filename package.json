{
  "name": "react-swiss",
  "version": "0.1.9",
  "description": "CSS-in-js with the best of the react and sass world together.",
  "main": "dist/cjs/index.js",
  "module": "dist/es/index.js",
  "scripts": {
    "prebuild": "rimraf dist/",
    "build": "node ./tools/build.js",
    "start": "npm-run-all --parallel start:swiss start:server",
    "start:swiss": "BABEL_ENV=es babel src -d dist/es --watch",
    "start:server": "cd ./tester && npm install && npm start",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/swipesapp/react-swiss.git"
  },
  "author": "Kasper Pihl Tornoe",
  "license": "MIT",
  "keywords": [
    "react",
    "css",
    "css-in-js",
    "styles",
    "sass",
    "scss",
    "less"
  ],
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-dev-expression": "^0.2.1",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "gzip-size": "^4.0.0",
    "npm-run-all": "^4.1.2",
    "pretty-bytes": "^4.0.2",
    "react": "^16.0.0",
    "rimraf": "^2.6.2",
    "rollup": "^0.50.0",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-commonjs": "^8.2.5",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-uglify": "^2.0.1",
    "uglify-es": "^3.1.5"
  },
  "peerDependencies": {
    "react": ">=15"
  },
  "dependencies": {
    "prop-types": "^15.6.0"
  }
}
